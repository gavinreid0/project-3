_LIST);\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n    try {\n      await add_post({\n        variables: { name: formState.name, description: formState.description, image: formState.image, price: formState.price },\n      });\n    } catch (e) {\n      console.log(e);\n    }\n  };\n\n  const handleChange = (event) => {\n    const { name, value } = event.target;\n    setFormState({\n      ...formState,\n      [name]: value,\n    });\n  };\n\n  return (\n    <div className=\"container my-1\">\n      <Link to=\"/list\">← Add Posting</Link>\n\n      <h2>List Posting</h2>\n      <form onSubmit={handleFormSubmit}>\n        <div className=\"flex-row space-between my-2\">\n          <label htmlFor=\"email\">Product Name:</label>\n          <input\n            placeholder=\"name your product\"\n            name=\"name\"\n            type=\"name\"\n            id=\"name\"\n            onChange={handleChange}\n          />\n        </div>\n        <div className=\"flex-row space-between my-2\">\n          <label htmlFor=\"description\">Description:</label>\n          <input\n            placeholder=\"description\"\n            name=\"description\"\n            type=\"description\"\n            id=\"description\"\n            onChange={handleChange}\n          />\n        </div>\n        <div className=\"flex-row space-between my-2\">\n          <label htmlFor=\"imgdesc\">Image:</label>\n          <input\n            placeholder=\"image\"\n            name=\"image\"\n            type=\"imagedesc\"\n            id=\"image\"\n            onChange={handleChange}\n          />\n        </div>\n        <div className=\"flex-row space-between my-2\">\n          <label htmlFor=\"price\">Price:</label>\n          <input\n            placeholder=\"price\"\n            name=\"price\"\n            type=\"price\"\n            id=\"price\"\n            onChange={handleChange}\n          />\n        </div>\n        <div className=\"flex-row flex-end\">\n          <button type=\"submit\">Submit</button>\n        </div>\n      </form>\n    </div>\n  );\n}\n\nexport default List;","/Users/rosas/Desktop/Web Development /SMU Bootcamp/Homework/project-3/client/src/pages/Profile.js",[],"/Users/rosas/Desktop/Web Development /SMU Bootcamp/Homework/project-3/client/src/components/ListForm/index.js",["115"],"/Users/rosas/Desktop/Web Development /SMU Bootcamp/Homework/project-3/client/src/components/ListList/index.js",[],{"ruleId":"116","replacedBy":"117"},{"ruleId":"118","replacedBy":"119"},{"ruleId":"120","severity":1,"message":"121","line":9,"column":22,"nodeType":"122","messageId":"123","endLine":9,"endColumn":27},{"ruleId":"120","severity":1,"message":"124","line":41,"column":15,"nodeType":"122","messageId":"123","endLine":41,"endColumn":19},"no-native-reassign",["125"],"no-negated-in-lhs",["126"],"no-unused-vars","'error' is assigned a value but never used.","Identifier","unusedVar","'data' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]